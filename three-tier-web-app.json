@{DATABASE_NAME}@@&#39;, &#39;@@{MYSQL_APP_USER}@@&#39;, &#39;@@{MYSQL_PASSWORD}@@&#39;, array(\n    PDO::ATTR_PERSISTENT =&gt; true,\n    PDO::ATTR_ERRMODE =&gt; PDO::ERRMODE_EXCEPTION,\n  ));\n\n    \/\/ ensure there is at least 1 row of data for the app to read\n    $data = $dbh-&gt;query(\&quot;INSERT INTO @@{DATABASE_TABLE}@@ (id, time, comment) VALUES (NULL, CURRENT_TIMESTAMP, \&quot;$_SERVER[&#39;SCRIPT_NAME&#39;]\&quot;)\&quot;);\n\n    $data = $dbh-&gt;query(&#39;SELECT MAX(id) AS count FROM @@{DATABASE_TABLE}@@&#39;)-&gt;fetchAll(PDO::FETCH_COLUMN);\n    $count = $data[0]; \/\/print_r($data);\n    $data = $dbh-&gt;query(\&quot;SELECT time, comment FROM @@{DATABASE_TABLE}@@ WHERE id = $count\&quot;)-&gt;fetchAll(PDO::FETCH_ASSOC);\n    $time = $data[0][&#39;time&#39;];\n    $comment = $data[0][&#39;comment&#39;];\n\n    echo \&quot;&lt;p&gt;Database records: $count&lt;\/p&gt;\\n\&quot;;\n    echo \&quot;&lt;p&gt;Last comment: $comment, on: $time&lt;\/p&gt;\\n\&quot;;\n    echo \&quot;&lt;h3&gt;Web docroot: $_SERVER[&#39;DOCUMENT_ROOT&#39;], file: $_SERVER[&#39;SCRIPT_NAME&#39;]&lt;\/h3&gt;\\n\&quot;;\n\n    $dbh = null;\n  }\n  catch (PDOException $e) {\n    print \&quot;Error!: \&quot; . $e-&gt;getcomment() . \&quot;&lt;br\/&gt;\&quot;;\n    print phpinfo();\n    die();\n  }\n  ?&gt;&lt;\/body&gt;\n  &lt;\/html&gt;\nEOF\n\n&quot;,&quot;script_type&quot;:&quot;sh&quot;,&quot;type&quot;:&quot;&quot;,&quot;command_line_args&quot;:&quot;&quot;,&quot;login_credential_local_reference&quot;:{&quot;kind&quot;:&quot;app_credential&quot;,&quot;name&quot;:&quot;teccadmin&quot;}},&quot;timeout_secs&quot;:&quot;0&quot;,&quot;type&quot;:&quot;EXEC&quot;,&quot;variable_list&quot;:[]}],&quot;description&quot;:&quot;&quot;,&quot;name&quot;:&quot;18bf19b0_runbook&quot;,&quot;main_task_local_reference&quot;:{&quot;kind&quot;:&quot;app_task&quot;,&quot;name&quot;:&quot;19fa8792_dag&quot;},&quot;variable_list&quot;:[]},&quot;type&quot;:&quot;&quot;,&quot;uninstall_runbook&quot